{
  "name": "paper-kit-portfolio",
  "version": "1.3.2",
  "dependencies": {
    "@fortawesome/fontawesome-free": "6.5.1",
    "ajv": "6.12.6",
    "ajv-keywords": "^3.5.2",
    "aos": "3.0.0-beta.6",
    "classnames": "2.3.3",
    "formik": "2.4.5",
    "nouislider": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.18.0",
    "react-scripts": "5.0.1",
    "reactstrap": "9.2.0",
    "sass": "1.66.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm update && npm start",
    "compile-sass": "sass src/assets/scss/paper-kit.scss src/assets/css/paper-kit.css",
    "minify-sass": "sass src/assets/scss/paper-kit.scss src/assets/css/paper-kit.min.css --style compressed",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "optionalDependencies": {
    "typescript": "5.2.2"
  },
  "devDependencies": {
    "@babel/plugin-transform-class-properties": "^7.25.4",
    "@babel/plugin-transform-optional-chaining": "^7.24.8",
    "bootstrap": "5.3.1",
    "gh-pages": "6.0.0",
    "mini-css-extract-plugin": "^2.9.1"
  },
  "main": "src/assets/scss/paper-kit.scss",
  "repository": {
    "type": "git",
    "url": "https://github.com/njwright92/paper-kit-portfolio.git"
  },
  "keywords": [
    "react",
    "reactstrap",
    "reactjs",
    "paper-kit",
    "paper-kit-react",
    "paper-kit-2",
    "paper-kit2",
    "paper-design",
    "hook",
    "hooks",
    "react-hook",
    "react-hooks",
    "template",
    "theme",
    "react-template",
    "react-theme"
  ],
  "author": "NathanWright",
  "license": "MIT",
  "homepage": "https://njwright92.github.io/paper-kit-portfolio"
}
